<% if user_signed_in? %>
  <li style="background-color:white;padding:2px">
    <form id="search-form">
      <input style="margin:4px 4px 2px 4px;width:220px" type="text" id="search" value="<%= @search %>" />
      <input type="submit" style="visibility:hidden;position:fixed" />
    </form>
  </li>
<% end %>
<% begin send("#{@module}_actions").reverse.each do |action| %>
  <li><%= nav_link_to action[:name], action[:path] %></li>
<% end; rescue; end %>
